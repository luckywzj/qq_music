.head_content{
    width:100%;
    height:100%;
    position:relative;
    background-image:-webkit-linear-gradient(bottom, #626262, #3e3c3a 80%)
}
.head_image{
    width:35%;
    height:230rpx
}
.body_content{
    margin:0 auto;
    padding-top:30rpx;
    width:90%;
    height:100%;
    display:flex
}
.rank{
    width:65%;
    height:100%;
    color:#fff;
    padding:30rpx 0 0 20rpx
}
button{
    margin-top:50rpx;
    margin-bottom:30rpx;
    width:50%;
    background:#31c27c;
    color:#fff;
    border-radius:50rpx
}
.rank>view{
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis ;
  margin-top:10rpx
}
 .line-css {
    height: 2rpx;
    background: #d9d9d9;
    width:100%;
    margin: 2rpx 0;
 }
 .list_item{
     display:flex;
      border-bottom:1px solid #d9d9d9
 }
 .list_num{
     padding:40rpx 10rpx 30rpx 50rpx;
    
 }
 .list_song{
     padding:15rpx;
 }